FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/solid";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 800.;

boundaryField
{
    sideEast
    {
        type            symmetryPlane;
    }
    sideWest
    {
        type            symmetryPlane;
    }
    faceSouth
    {
        type            zeroGradient;
    }
    faceNorth
    {
        type            zeroGradient;
    }
    solid_to_fluid
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           uniform 800.;
        Tnbr            T;
    }

    axis
    {
        type            symmetryPlane;
    }
}


