FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system/solid";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    e
    {
        solver          smoothSolver;
        smoother        GaussSeidel;
        nSweeps         2;
        tolerance       1e-06;
        relTol          1e-3;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 	0;

    residualControl
    {
       	e	1e-8;
    }
}

relaxationFactors
{
    equations
    {
	e	0.3;
    }
}

